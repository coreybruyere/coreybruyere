{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "A modern WordPress stack",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "roots", "wordpress", "stack", "composer", "vagrant", "wp"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "deliciousbrains/wp-migrate-db-pro",
        "type": "wordpress-plugin",
        "version": "1.5",
        "dist": {
          "type": "zip",
          "url": "https://deliciousbrains.com/dl/wp-migrate-db-pro-latest.zip?licence_key=fee715cd-2ccb-498f-92f3-4758432e04ff&site_url=composer-test.dev"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.5",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.3.1",
    "roots/soil": "3.4.0",
    "deliciousbrains/wp-migrate-db-pro": "*",
    "wpackagist-plugin/advanced-custom-fields":"4.4.3",
    "wpackagist-plugin/custom-post-type-ui":"1.1.2",
    "wpackagist-plugin/regenerate-thumbnails":"2.2.4",
    "wpackagist-plugin/w3-total-cache":"0.9.4.1",
    "wpackagist-plugin/updraftplus":"1.11.15",
    "wpackagist-plugin/the-events-calendar":"3.12.3",
    "wpackagist-plugin/wp-smushit":"2.0.7",
    "wpackagist-plugin/wordpress-importer":"0.6.1",
    "wpackagist-plugin/wordfence":"6.0.20",
    "wpackagist-plugin/wp-retina-2x":"3.5.4"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
